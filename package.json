{
    "name": "vscode-jape",
    "displayName": "vscode-jape",
    "description": "jape syntax highlight",
    "version": "0.0.1",
    "publisher": "lt",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jape",
            "extensions": [".jape"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jape",
            "scopeName": "text.jape",
            "path": "./syntaxes/jape.tmLanguage.json"
        }]
    }
}
